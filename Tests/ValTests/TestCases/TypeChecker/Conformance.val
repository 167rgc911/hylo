view V {
  new()
  fun foo()
  fun bar(a: A) -> B
}

type A: V {
  new() {}
  fun foo() {}
  fun bar(a: A) -> B {
    ret B(a: self)
  }
}

// #!error@+2 conformance to 'V' requires a matching implementation of 'new'
// #!error@+1 conformance to 'V' requires a matching implementation of 'bar'
type B: V {
  new(a: A) {}
  fun foo() {}
}
