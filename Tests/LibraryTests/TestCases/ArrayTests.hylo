//- compileAndRun expecting: success

fun test_init_empty() {
  var d = Array<Int>()
  precondition(d.count() == 0)
}

fun test_append() {
  var d = Array<Int>()
  &d.append(21)
  &d.append(42)
  &d.append(84)

  precondition(d[0] == 21)
  precondition(d[1] == 42)
  precondition(d[2] == 84)
}

fun test_swap_at() {
  var a = Array<Bool>()
  &a.append(false)
  &a.append(true)
  &a.swap_at(0, 1)
  precondition(a[0])
}

public fun main() {
  test_init_empty()
  test_append()
  test_swap_at()
}
