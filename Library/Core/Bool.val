/// A value that can be either `true` or `false`.
public type Bool {

  var value: Builtin.i1

  memberwise init

  /// Initializes a instance with the value of `0`.
  public init() {
    self.value = Builtin.zeroinitializer_i1()
  }

}

public conformance Bool: Copyable {

  public fun copy() -> Self {
    Bool(value: value)
  }

}
