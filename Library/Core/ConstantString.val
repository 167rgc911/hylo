/// A Unicode string.
public type ConstantString {

  /// The size of `contents` in bytes.
  public var size: Int

  /// The contents of the string, encoded in UTF8.
  public var utf8: RawPointer

}

public conformance ConstantString: Copyable {

  public fun copy() -> Self {
    ConstantString(size: size.copy(), utf8: utf8.copy())
  }

}
