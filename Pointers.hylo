fun test(_ p: Pointer<Int>, _ m: PointerToMutable<Int>) {
  var x: Int = p.unsafe[].copy()
  x += m.unsafe[]
  var m1 = m.copy()
  m1.unsafe[] += x
  m1.unsafe[] += p.unsafe[]
}

public fun main() {
  let x = 3
  var y = 4
  test(pointer[to: x], pointer[toMutable: &y])
  precondition(y == 14)
}
